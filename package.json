{
    "name": "serviser-sdk",
    "version": "1.4.0",
    "description": "Exposes common sdk interface & provides client sdk generator for serviser based apps",
    "main": "index.js",
    "browser": "browser.js",
    "bin": "./bin/serviser-sdk.js",
    "scripts": {
        "test": "export NODE_ENV=test; ./node_modules/mocha/bin/_mocha --recursive --ui tdd -t 32000 -R spec tests/acceptance tests/unit",
        "coverage": "export NODE_ENV=test; istanbul cover -x ./bin/serviser-sdk.js node_modules/mocha/bin/_mocha -- --recursive --ui tdd -t 16000 -R spec tests/acceptance tests/unit"
    },
    "testFramework": "mocha",
    "keywords": [
        "client",
        "serviser",
        "service",
        "interface",
        "sdk",
        "API"
    ],
    "engines": {
        "node": ">=6.4.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lucid-services/serviser-sdk"
    },
    "files": [
        "bin",
        "lib",
        "index.js",
        "browser.js"
    ],
    "license": "GPL-3.0",
    "author": "fogine",
    "private": false,
    "dependencies": {
        "axios": "^0.16.x",
        "lodash.camelcase": "4.3.0",
        "tmp": "0.0.31",
        "jshint": "^2.9.4",
        "mustache": "^2.3.0",
        "yargs": "^8.0.0",
        "bluebird": "^3.5.0",
        "archiver": "^1.3.0"
    },
    "devDependencies": {
        "serviser-doc": "^2.1.2",
        "serviser": "^2.0.0",
        "serviser-config": "^3.0.0",
        "chai": "^4.1.2",
        "chai-as-promised": "^7.1.1",
        "istanbul": "^0.4.3",
        "lodash": "^4.15.0",
        "mocha": "^3.5.3",
        "sinon": "^1.17.3",
        "sinon-as-promised": "^4.0.0",
        "sinon-chai": "^2.8.0"
    }
}
